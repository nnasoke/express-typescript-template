{
  "name": "express-typescript-template",
  "version": "1.0.0",
  "description": "Express and typescript template",
  "main": "index.js",
  "author": "nnasoke",
  "license": "MIT",
  "private": true,
  "scripts": {
    "typeorm": "ts-node ./node_modules/typeorm/cli.js --config src/configs/database.ts",
    "migrate:generate": "yarn typeorm migration:generate -n",
    "migrate:create": "yarn typeorm migration:create -n",
    "migrate:run": "yarn typeorm migration:run",
    "migrate:revert": "yarn typeorm migration:revert",
    "test": "yarn lint && intern",
    "build": "tsc",
    "start:dev": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/index.ts",
    "start:prod": "node ./dist",
    "lint": "tslint --project \"tsconfig.json\""
  },
  "devDependencies": {
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.7",
    "@types/morgan": "^1.9.1",
    "@types/supertest": "^2.0.10",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "intern": "^4.8.7",
    "nodemon": "^2.0.4",
    "prettier": "^2.0.5",
    "supertest": "^4.0.2",
    "ts-node": "^8.10.2",
    "tslint": "^6.1.2",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.9.7"
  },
  "dependencies": {
    "@types/node": "^14.0.27",
    "@types/pg": "^7.14.4",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "http-status-codes": "^1.4.0",
    "morgan": "^1.10.0",
    "pg": "^8.3.0",
    "reflect-metadata": "^0.1.13",
    "sqlite3": "^5.0.0",
    "typeorm": "^0.2.25",
    "winston": "^3.3.3"
  }
}
